{
  "compilerOptions": {
    "lib": ["esnext"],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "jsx": "react",
    "jsxFactory": "h"
  }
}
