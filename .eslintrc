{
  "env": {
    "browser": true,
    "es6": true,
  },
  "extends": [
    "airbnb",
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "rules": {
    "semi": ["error", "never"],
    "arrow-parens": ["error", "always"],
    "new-cap": "off",
    "capitalized-comments": "off",
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
  },
}
