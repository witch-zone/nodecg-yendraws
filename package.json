{
  "description": "A cute and good stream layout for yen!",
  "keywords": [
    "nodecg-bundle"
  ],
  "repository": "https://github.com/witch-zone/nodecg-yendraws.git",
  "homepage": "http://twitch.tv/yendraws",
  "name": "nodecg-yendraws",
  "version": "1.0.0",
  "author": {
    "name": "dani quinn sexton",
    "email": "dani@glitter.tech",
    "url": "http://glitter.tech"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "nodecg": {
    "compatibleRange": ">=1.6.1",
    "bundleDependencies": {
      "nodecg-twitchie": "^3.0.0"
    },
    "dashboardPanels": [
      {
        "name": "timer",
        "title": "Timer",
        "width": 2,
        "file": "timer.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "Commands",
        "title": "Chat Commands",
        "width": 2,
        "file": "commands.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "Manage Commands",
        "title": "Manage Commands",
        "width": 2,
        "file": "manage-commands.html",
        "workspace": "Config"
      },
      {
        "name": "Manage Schedule",
        "title": "Manage Schedule",
        "width": 2,
        "file": "manage-schedule.html",
        "workspace": "Config"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      }
    ]
  },
  "scripts": {
    "build": "npm run build:dashboard && npm run build:graphics",
    "build:dashboard": "webpack --config webpack.config.dashboard.js",
    "build:graphics": "webpack --config webpack.config.graphics.js",
    "lint": "eslint \"src/**/*.{ts,tsx,js,tsx}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/color": "^3.0.1",
    "@types/react-motion": "^0.0.32",
    "@types/react-redux": "^7.1.22",
    "@types/twemoji": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.6.0",
    "esbuild-loader": "2.18.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.5.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "svg-sprite-loader": "^6.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@polymer/iron-icon": "^3.0.0",
    "@polymer/iron-icons": "^3.0.0",
    "@polymer/iron-list": "^3.0.0",
    "@polymer/iron-pages": "^3.0.0",
    "@polymer/paper-button": "^3.0.0",
    "@polymer/paper-checkbox": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.0",
    "@polymer/paper-input": "^3.0.0",
    "@polymer/polymer": "^3.0.0",
    "@types/react-motion": "0.0.29",
    "classnames": "^2.3.1",
    "moment": "^2.24.0",
    "nodecg-twitchie": "^3.2.1",
    "nodecg-twitchie-graphics": "^4.0.0",
    "normalize.css": "^8.0.1",
    "preact": "^10.6.5",
    "react-motion": "^0.5.2",
    "twemoji": "^12.1.5",
    "velocity-animate": "^1.5.2",
    "xjs-framework": "^2.9.0",
    "zustand": "^3.7.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{ts,tsx,scss,json,md,html}": [
      "prettier --write"
    ]
  }
}
