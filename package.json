{
  "name": "nodecg-yendraws",
  "version": "1.0.0",
  "description": "A cute and good stream layout for yen!",
  "homepage": "http://twitch.tv/yendraws",
  "author": {
    "name": "dani quinn sexton",
    "email": "dani@glitter.tech",
    "url": "http://glitter.tech"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": ">=1.0.0",
    "bundleDependencies": {
      "nodecg-twitchie": "^2.0.0"
    },
    "dashboardPanels": [
      {
        "name": "Commands",
        "title": "Chat Commands",
        "width": 2,
        "file": "commands.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "Manage Commands",
        "title": "Manage Commands",
        "width": 2,
        "file": "manage-commands.html",
        "workspace": "Config"
      },
      {
        "name": "Manage Schedule",
        "title": "Manage Schedule",
        "width": 2,
        "file": "manage-schedule.html",
        "workspace": "Config"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      }
    ]
  },
  "repository": "https://github.com/witch-zone/nodecg-yendraws.git",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/react-redux": "^7.1.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "img-loader": "^3.0.1",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.15.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.11.1",
    "svg-sprite-loader": "^4.1.6",
    "terser-webpack-plugin": "^2.1.0",
    "ts-loader": "^6.1.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@types/react-motion": "0.0.29",
    "classnames": "^2.2.6",
    "nodecg-twitchie": "2.0.3",
    "nodecg-twitchie-graphics": "2.0.6",
    "preact": "^10.0.1",
    "preact-context": "^1.1.4",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "twemoji": "^12.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{ts,tsx,scss,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
