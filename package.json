{
  "description": "A cute and good stream layout for yen!",
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "repository": "https://github.com/witch-zone/nodecg-yendraws.git",
  "homepage": "http://twitch.tv/yendraws",
  "name": "nodecg-yendraws",
  "version": "1.0.0",
  "author": {
    "name": "dani quinn sexton",
    "email": "dani@glitter.tech",
    "url": "http://glitter.tech"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "nodecg": {
    "compatibleRange": ">=1.5.0",
    "bundleDependencies": {
      "nodecg-twitchie": "^2.3.0"
    },
    "dashboardPanels": [
      {
        "name": "Commands",
        "title": "Chat Commands",
        "width": 2,
        "file": "commands.html",
        "headerColor": "#6441A4"
      },
      {
        "name": "Manage Commands",
        "title": "Manage Commands",
        "width": 2,
        "file": "manage-commands.html",
        "workspace": "Config"
      },
      {
        "name": "Manage Schedule",
        "title": "Manage Schedule",
        "width": 2,
        "file": "manage-schedule.html",
        "workspace": "Config"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      }
    ]
  },
  "scripts": {
    "build": "npm run build:dashboard && npm run build:graphics",
    "build:dashboard": "webpack --config webpack.config.dashboard.js",
    "build:graphics": "webpack --config webpack.config.graphics.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/react-redux": "^7.1.22",
    "@types/twemoji": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^7.0.4",
    "img-loader": "^3.0.1",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.15.3",
    "sass": "^1.49.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.11.1",
    "svg-sprite-loader": "^4.1.6",
    "terser-webpack-plugin": "^2.1.0",
    "ts-loader": "^6.1.2",
    "typescript": "^4.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@polymer/iron-icons": "^3.0.0",
    "@polymer/iron-list": "^3.0.0",
    "@polymer/paper-button": "^3.0.0",
    "@polymer/paper-icon-button": "^3.0.0",
    "@polymer/paper-input": "^3.0.0",
    "@polymer/polymer": "^3.0.0",
    "@types/react-motion": "0.0.29",
    "classnames": "^2.3.1",
    "moment": "^2.24.0",
    "nodecg-twitchie": "^3.0.0",
    "nodecg-twitchie-graphics": "^3.0.0",
    "preact": "^10.6.5",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.6",
    "redux": "^4.0.4",
    "twemoji": "^12.1.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{ts,tsx,scss,json,md,html}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  }
}
